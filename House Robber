class Solution {
    public int Rob(int[] nums) {
        if(nums == null)
            return -1;
        else if(nums.length== 0)
            return 0;
        else if(nums.length== 2)
            return Math.max(nums[0],nums[1]);
        else{
            int prev = nums[0];
		    int prevprev = 0;
		    int max = prev;
		    for (int i = 1; i < nums.length; i++)
            {
                max = Math.max(nums[i] + prevprev, prev);
                prevprev = prev;
                prev = max;
            }
            return max;
        
    }
}}
