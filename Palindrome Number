public class Solution 
{
    public bool IsPalindrome(int x) 
    {
        if(x<0)
            return false;
        int res=0; int rem = x;
        
        while(rem>0)
        {
            
            res = res*10 + rem%10;  
            rem /= 10; 
            
        }
        if(x == res)
          return true;
        else 
          return false;
         
       
        
    }
}
